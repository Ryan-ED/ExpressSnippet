{
    "name": "expresssnippet",
    "displayName": "ExpressSnippet",
    "description": "Express Snippets for VS Code",
    "version": "0.0.3",
    "publisher": "vladmrnv",
    "homepage": "https://github.com/vladmrnv/ExpressSnippet",
    "icon": "images/icon.svg",
    "repository": {
        "type": "git",
        "url": "https://github.com/vladmrnv/ExpressSnippet"
    },
    "engines": {
        "vscode": "^0.10.10"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/javascript.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}